package OHOS.testserver;

struct ProcessMemoryInfo {
    int pid;
    String name;
    int rss;
    int pss;
    int swapPss;
    int adj;
    int sharedDirty;
    int privateDirty;
    int sharedClean;
    int privateClean;
    int procState;
};

struct ProcessCpuInfo {
    unsigned long startTime;
    unsigned long endTime;
    int pid;
    unsigned int minFlt;
    unsigned int majFlt;
    double cpuLoad;
    double uCpuUsage;
    double sCpuUsage;
    double cpuUsage;
    String procName;
    int threadCount;
};